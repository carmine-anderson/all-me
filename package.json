    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.30.10",
    "framer-motion": "^11.0.28",
    "postgres": "^3.4.4",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hook-form": "^7.51.2",
    "react-hot-toast": "^2.4.1",
